        <file alias="reorder.svg">images/reorder.svg</file>
		<file alias="add.svg">images/add-black.svg</file>
		<file alias="delete.svg">images/delete.svg</file>
		<file alias="edit.svg">images/edit.svg</file>
		<file alias="backspace.svg">images/backspace.svg</file>
    </qresource>
